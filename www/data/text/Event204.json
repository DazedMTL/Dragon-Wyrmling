    "12": {
        "name": "Isk",
        "face": "喜2",
        "text": "\"I really like it...\""
    },
    "13": {
        "name": "Isk",
        "face": "喜3",
        "text": "\"Eh... this is bad...\""
    },
    "14": {
        "name": "Isk",
        "face": "喜2",

    "22": {
        "name": "Isk",
        "face": null,
        "text": "\"Everyone... Ah... so very robust...\""
    },
    "23": {
        "name": "Sol",
        "face": null,
        "text": "\"Ehehe... Please, take good care of me tonight\ntoo, okay?\""
    },
    "24": {
        "name": "Isk",
        "face": null,
        "text": "\"Being able to assist the hero... is the greatest\nhappiness of all...\""
    },
    "25": {
        "name": "Sol",
        "face": null,
        "text": "\"Everyone, I love you all so much...♡\""
